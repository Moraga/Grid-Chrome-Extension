{
	"name": "Grid",
	"version": "0.1",
	"manifest_version": 2,
	"browser_action": {
		"default_icon": "icon.png"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_scripts": [{
		"matches": ["http://*/*", "https://*/*", "file://*/*"],
		"js": ["content.js"]
	}],	
	"permissions": ["activeTab"]
}